package repetitionAndSelection;

public class LoopExercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int number = 2; number < 100; number++){
			number = number * 2;
			if (number < 15)
				System.out.println("Number is less then 15: " + number);
			else if (number > 15)
				System.out.println("Number is greater then 15: " + number);
		}
		

	}

}
