package repetitionAndSelection;

public class LoopExercise3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		
		do {
			if ((i % 2) == 0)
				System.out.println("((i % 2) == 0)= " + i);
			else
				System.out.println("((i % 2) != 0)= " + i);
			i++;
		}while (i < 10);
		

	}

}
